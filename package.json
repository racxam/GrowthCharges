{
    "name": "dashboard",
    "version": "0.0.1",
    "private": true,
    "description": "A Fiori application.",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "devDependencies": {
        "@babel/eslint-parser": "7.14.7",
        "@sap-ux/ui5-middleware-fe-mockserver": "2",
        "@sap/ux-specification": "UI5-1.71",
        "@sap/ux-ui5-tooling": "^1.9.0",
        "@ui5/cli": "^4.0.19",
        "@wdio/cli": "^9.17.1",
        "@wdio/local-runner": "^9.17.0",
        "@wdio/mocha-framework": "^7.26.0",
        "@wdio/spec-reporter": "^9.17.0",
        "chromedriver": "^138.0.2",
        "eslint": "7.32.0",
        "eslint-plugin-fiori-custom": "2.2.1",
        "rimraf": "^3.0.2",
        "ui5-task-flatten-library": "0.2.5",
        "wdio-chromedriver-service": "^8.1.1",
        "wdio-ui5-service": "^1.2.0"
    },
    "scripts": {
        "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#comgcdashboard-tile\"",
        "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#comgcdashboard-tile\"",
        "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
        "lint": "eslint ./ --rulesdir ./node_modules/eslint-plugin-fiori-custom/lib/rules/",
        "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
        "deploy-config": "fiori add deploy-config",
        "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
        "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#comgcdashboard-tile\"",
        "start-variants-management": "fiori run --open \"preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
        "undeploy": "fiori undeploy --config ui5-deploy.yaml",
        "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true",
        "wdi5": "wdio run wdio.conf.js"
    },
    "ui5": {
        "dependencies": [
            "@sap/ux-ui5-tooling",
            "@sap-ux/ui5-middleware-fe-mockserver",
            "ui5-task-flatten-library"
        ]
    },
    "sapux": true,
    "sapuxLayer": "CUSTOMER_BASE"
}